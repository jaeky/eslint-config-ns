{
  "name": "eslint-config-ns",
  "version": "1.0.0",
  "description": "eslint config ready to be used in multiple projects",
  "author": "Stefan Natter",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "bugs": {
    "url": "https://github.com/natterstefan/eslint-config-ns/issues"
  },
  "homepage": "https://github.com/natterstefan/eslint-config-ns#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/natterstefan/eslint-config-ns"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "prettier.js",
    "prettier.config.js"
  ],
  "scripts": {
    "start": "cd example && npm start",
    "test": "(cd example && npm run lint && npm t)"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "babel-eslint": ">=10.0.3 <11",
    "eslint": ">=6.8.0 <7",
    "eslint-config-airbnb": ">=18.0.1 <19",
    "eslint-config-prettier": ">=6.9.0 <7",
    "eslint-plugin-import": ">=2.19.1 <3",
    "eslint-plugin-jest": ">=23.2.0 <24",
    "eslint-plugin-jsx-a11y": ">=6.2.3 <7",
    "eslint-plugin-prettier": ">=3.1.2 <4",
    "eslint-plugin-react": ">=7.17.0 <8",
    "eslint-plugin-react-hooks": ">=2.3.0 <3",
    "prettier": ">=1.19.1 <2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.2.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "^3.1.0",
    "prettier": "1.19.1"
  }
}
